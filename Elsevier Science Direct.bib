}

@article{konigsrainerPreservingDuodenalPassage2009,
  title = {Preserving {{Duodenal Passage}} for {{Bone Mineralization}}: {{Billroth I}} versus {{Billroth II Reconstruction After Partial Gastrectomy}} in {{Growing Minipigs}}},
  shorttitle = {Preserving {{Duodenal Passage}} for {{Bone Mineralization}}},
  author = {K{\"o}nigsrainer, Ingmar and K{\"o}nigsrainer, Alfred and Maier, Gerhard W.},
  year = {2009},
  month = aug,
  journal = {Journal of Surgical Research},
  volume = {155},
  number = {2},
  pages = {321--329},
  issn = {0022-4804},
  doi = {10.1016/j.jss.2008.07.026},
  abstract = {Introduction Recent data in morbid obesity surgery have shown that the Roux-en-Y gastric bypass results in a decrease in bone mass. Therefore, we investigated the impact of duodenal exclusion on bone mineralization in respect to newer duodenal preserving operations in morbid obesity surgery. Methods Twelve female minipigs underwent Billroth I (n = 6) or Billroth II (n = 6) reconstruction. Serum parameters were investigated and bone mineral density of the fourth lumbar vertebra was evaluated by quantitative CT scan at monthly intervals over a 1-y period. Calcium absorption was measured as 47calcium-carbonate at 3-mo intervals. Results At the end of the study, the cancellous volumetric bone mineral density (cancvBMD) and the cortical volumetric bone mineral density (cortvBMD) were significantly more decreased in Billroth II (BII) than in Billroth I (BI) reconstructed minipigs (371.86 {$\pm$} 1.61 versus 386.96 {$\pm$} 5.01 mg/cm3; 402.22 {$\pm$} 1.64 versus 410.7 {$\pm$} 2.08 mg/cm3; both P {$<$} 0.05). Similar distribution was measured for cancellous and cortical bone mineral content (670.0 {$\pm$} 7.5 versus 712.8 {$\pm$} 11.3 mg; 682.2 {$\pm$} 40.5 versus 717.8 {$\pm$} 43.4 mg; both P {$<$} 0.05). Medullary and cortical areas were significantly decreased in BII pigs compared with BI pigs. 47Ca absorption was significantly decreased in BII 3 and 6 mo following the operation, then reached values similar to those in BI animals. Conclusion Exclusion of the duodenum from the passage resulted in impaired mineralization of the axial skeleton due to decreased calcium absorption and impaired vertebral bone volume.},
  langid = {english},
  keywords = {Billroth I-Billroth II reconstruction,bone mineral density,minipigs,partial gastrectomy},
  file = {D\:\\Zotero\\storage\\WMKN2DX4\\Königsrainer et al. - 2009 - Preserving Duodenal Passage for Bone Mineralizatio.pdf;D\:\\Zotero\\storage\\ZU9SRDYA\\S0022480408004903.html}
}

@article{lindhCellBiologyTrace1995,
