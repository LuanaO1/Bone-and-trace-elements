@article{dreesenDevelopmentQualityCare2013,
  title = {Development of Quality of Care Interventions for Adult Patients on Home Parenteral Nutrition ({{HPN}}) with a Benign Underlying Disease Using a Two-Round {{Delphi}} Approach},
  author = {Dreesen, Mira and Foulon, Veerle and Vanhaecht, Kris and Hiele, Martin and De Pourcq, Lutgart and Pironi, Loris and Van Gossum, Andre and Wanten, Geert and Baxter, Janet P. and Joly, Francisca and Cuerda, Cristina and Willems, Ludo},
  year = {2013},
  month = feb,
  journal = {Clinical Nutrition},
  volume = {32},
  number = {1},
  pages = {59--64},
  issn = {0261-5614},
  doi = {10.1016/j.clnu.2012.05.006},
  abstract = {Background \& aims HPN patients with benign diseases deserve professional care as they have to deal with complex techniques and risk potentially dangerous complications. The aim was to highlight main outcome quality indicators and to develop a set of key interventions to direct multidisciplinary teams in providing qualitative care. Methods A two-round Delphi approach was used to build consensus on the most important outcome indicators and on 59 interventions identified in existing guidelines on HPN. Comments and interventions newly identified in the first round were co-evaluated in the second round. Results 29 experts from 9 countries completed the two-round Delphi approach. The outcome indicators rated as the most important are 1) incidence of catheter-related infections, 2) incidence of readmission and quality of life (shared second place) and 3) incidence of dehydration. Sixty eight of a total of 89 interventions were considered as important for the quality of care, of which 46 are based on published guidelines and 22 were newly suggested by the Delphi panel. Conclusions Using a two-round Delphi approach, consensus was reached for the majority of interventions concerning HPN patients with benign diseases. This set of 68 interventions could be of use as a starting point for quality-improvement programs.},
  langid = {english},
  keywords = {Adult patients,Benign underlying disease,Care pathway,Home parenteral nutrition,Quality of care},
  file = {D\:\\Zotero\\storage\\XK442HYK\\Dreesen et al. - 2013 - Development of quality of care interventions for a.pdf;D\:\\Zotero\\storage\\EB5FVBSP\\S0261561412001069.html}
}

@article{falchSecularIncreaseGeographical1993,
  title = {Secular Increase and Geographical Differences in Hip Fracture Incidence in {{Norway}}},
  author = {Falch, J. A. and Kaastad, T. S. and B{\o}hler, G. and Espeland, J. and Sundsvold, O. -J.},
  year = {1993},
  month = jul,
  journal = {Bone},
  volume = {14},
  number = {4},
  pages = {643--645},
  issn = {8756-3282},
  doi = {10.1016/8756-3282(93)90086-P},
  abstract = {In 1988 and 1989, 2790 hip fractures were reported from the urban population of Oslo (annual incidence per 10 000 in the age group 50 years and older: women 118.7, men 45.4). For the same period, 385 fractures were reported from the rural population of the county Sogn og Fjordane (annual incidence per 10 000 in the age-group 50 years and older: women 74.6, men 36.7). The hip fracture incidence in Oslo had increased for all sex and age groups above 50 years (except women 50\textendash 59 years) in the 10-year period 1978/79 to 1988/89. The incidence of hip fractures in Sogn og Fjordane was only 65\% of the incidence in Oslo. These results support previous reports on a secular increase and geographical differences in hip fracture incidence.},
  langid = {english},
  keywords = {Epidemiology,Geography,Hip fracture,Incidence,Osteoporosis},
  file = {D\:\\Zotero\\storage\\R3CCISLL\\Falch et al. - 1993 - Secular increase and geographical differences in h.pdf;D\:\\Zotero\\storage\\6TJM5BV2\\875632829390086P.html}
}

@article{frostCoherenceTreatmentOsteoporoses1981,
  title = {Coherence {{Treatment}} of {{Osteoporoses}}},
  author = {Frost, H. M.},
  year = {1981},
  month = jul,
  journal = {Orthopedic Clinics of North America},
  volume = {12},
  number = {3},
  pages = {649--669},
  issn = {0030-5898},
  doi = {10.1016/S0030-5898(20)31376-6},
  abstract = {No agent yet found when given continuously to the intact, temporally incoherent adult human skeleton can store in it progressively increasing amounts of bone. BMU theory plus clinical observation suggest that such failure derives from approximately equal effects of such agents on both resorptive and formative activity, partly by affecting the naturally coupled activities of already existing osteoclast and osteoblast populations in BMU and partly by affecting BMU activation. However, the BMU theory also suggests that special sequences of therapeutic manipulations can create a positive bone balance in repeatable increments. One such proposal, called the ADFR sequence, involves first briefly pulsing BMU activation. The resorptive and formative activities created by that pulse will occur in step but at predictably different times, that is, they become temporally coherent. Depressing the coherent resorption with a suitable agent during its lifetime, then freeing the skeleton of treatment during the following coherent osteoblastic activity should cause BMU to deposit normal amounts of new bone in smaller than normal holes, resulting in a net surplus or increment. Repeating the sequence should add further increments. Other manipulations of coherent bone cell populations could remove bone or simply alter its turnover speed, as desired. Thus, special sequences of simple pharmacologic actions that have no useful ultimate effects upon global bone balance when delivered continuously and alone to the incoherent skeleton could create a simple new kind of therapy in the coherent skeleton. The concept of manipulating deliberately induced coherent states for therapeutic purposes should generalize to the treatment of many other tissues, cells, and organs that have the requisite properties. Accepting the encouragement provided by early human and animal trials of the idea, time will tell whether it will join numerous disappointing predecessors reviewed elsewhere in this issue or if it will indeed mark a turn away from them toward a more fruitful approach to treatment and prevention.},
  langid = {english},
  file = {D\:\\Zotero\\storage\\Y3GKWXEJ\\S0030589820313766.html}
}

@article{galvez-fernandezArsenicCadmiumSelenium2021,
  title = {Arsenic, Cadmium, and Selenium Exposures and Bone Mineral Density-Related Endpoints: {{The HORTEGA}} Study},
  shorttitle = {Arsenic, Cadmium, and Selenium Exposures and Bone Mineral Density-Related Endpoints},
  author = {{Galvez-Fernandez}, Marta and {Grau-Perez}, Maria and {Garcia-Barrera}, Tamara and {Ramirez-Acosta}, Sara and {Gomez-Ariza}, Jose L. and {Perez-Gomez}, Beatriz and {Galan-Labaca}, I{\~n}aki and {Navas-Acien}, Ana and Redon, Josep and {Briongos-Figuero}, Laisa S. and {Due{\~n}as-Laita}, Antonio and {Perez-Castrillon}, Jose Luis and {Tellez-Plaza}, Maria and {Martin-Escudero}, Juan Carlos},
  year = {2021},
  month = jan,
  journal = {Free Radical Biology and Medicine},
  volume = {162},
  pages = {392--400},
  issn = {0891-5849},
  doi = {10.1016/j.freeradbiomed.2020.10.318},
  abstract = {Background and objectives Experimental data suggest that trace elements, such as arsenic (As), cadmium (Cd), and selenium (Se) can influence the bone remodeling process. We evaluated the cross-sectional association between As, Cd, and Se biomarkers with bone mineral density (BMD) measured at the calcaneus, in a representative sample of a general population from Spain. As secondary analyses we evaluated the associations of interest in subgroups defined by well-established BMD determinants, and also conducted prospective analysis of osteoporosis-related incident bone fractures restricted to participants older than 50 years-old. Methods In N~=~1365 Hortega Study participants {$>$}20 years-old, urine As and Cd were measured by inductively coupled-plasma mass spectrometry (ICPMS); plasma Se was measured by atomic absorption spectrometry (AAS) with graphite furnace; and BMD at the calcaneus was measured using the Peripheral Instaneuous X-ray Imaging system (PIXI). As levels were corrected for arsenobetaine (Asb) to account for inorganic As exposure. Results The median of total urine As, Asb-corrected urine As, urine Cd, and plasma Se was 61.3, 6.53 and 0.39~{$\mu$}g/g creatinine, and 84.9~{$\mu$}g/L, respectively. In cross-sectional analysis, urine As and Cd were not associated with reduced BMD (T-score~{$<~$}-1 SD). We observed a non-linear dose-response of Se and reduced BMD, showing an inverse association below \textasciitilde 105~{$\mu$}g/L, which became increasingly positive above \textasciitilde 105~{$\mu$}g/L. The evaluated subgroups did not show differential associations. In prospective analysis, while we also observed a U-shape dose-response of Se with the incidence of osteoporosis-related bone fractures, the positive association above \textasciitilde 105~{$\mu$}g/L was markedly stronger, compared to the cross-sectional analysis. Conclusions Our results support that Se, but not As and Cd, was associated to BMD-related disease. The association of Se and BMD-related disease was non-linear, including a strong positive association with osteoporosis-related bone fractures risk at the higher Se exposure range. Considering the substantial burden of bone loss in elderly populations, additional large prospective studies are needed to confirm the relevance of our findings to bone loss prevention in the population depending on Se exposure levels.},
  langid = {english},
  keywords = {Arsenic,Bone mineral density,Cadmium,Osteoporosis,Selenium},
  file = {D\:\\Zotero\\storage\\VPY45KYG\\S0891584920315926.html}
}

@article{gambertOsteoporosisClinicalFeatures1995,
  title = {Osteoporosis: {{Clinical Features}}, {{Prevention}}, and {{Treatment}}},
  shorttitle = {Osteoporosis},
  author = {Gambert, Steven R. and Schultz, Barry M. and Hamdy, Ronald C.},
  year = {1995},
