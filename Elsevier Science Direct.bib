@incollection{hinghofer-szalkayHumanNutritionExtraterrestrial1992,
  title = {Human {{Nutrition Under Extraterrestrial Conditions}}},
  booktitle = {Advances in {{Space Biology}} and {{Medicine}}},
  author = {{Hinghofer-Szalkay}, Helmut G. and K{\"o}nig, Eva M.},
  editor = {Bonting, Sjoerd L.},
  year = {1992},
  month = jan,
  series = {Advances in {{Space Biology}} and {{Medicine}}},
  volume = {2},
  pages = {131--179},
  publisher = {{Elsevier}},
  doi = {10.1016/S1569-2574(08)60020-9},
  abstract = {This chapter discusses the human nutrition under extraterrestrial conditions. Specific nutrients are particularly important for certain physiological functions; they are dependent upon the presence of other nutrients for optimal effect. Thus, no single substance by itself can maintain the proper functioning of any biological subsystem; rather, a balanced intake of all necessary nutrients is needed for optimal nutrition of astronauts. Short-term periods of spaceflight (weeks) present no serious problems in terms of nutritional surveillance and planning; eventual body weight losses and energy deficits can easily be sustained on short missions. However, even an increased food supply does not entirely prevent an inflight weight loss. The chapter also reviews the various types and availability of nutrients. The major part of the human diet is composed of macronutrients\textemdash water, proteins, carbohydrates, fat, and nucleic acids.},
  langid = {english},
  file = {D\:\\Zotero\\storage\\RRDFKIZR\\S1569257408600209.html}
}

@article{honkanenDoesLactoseIntolerance1996,
  title = {Does Lactose Intolerance Predispose to Low Bone Density? {{A}} Population-Based Study of Perimenopausal Finnish Women},
  shorttitle = {Does Lactose Intolerance Predispose to Low Bone Density?},
  author = {Honkanen, R. and Pulkkinen, P. and J{\"a}rvinen, R. and Kr{\"o}ger, H. and Lindstedt, K. and Tuppurainen, M. and Uusitupa, M.},
  year = {1996},
  month = jul,
  journal = {Bone},
  volume = {19},
  number = {1},
  pages = {23--28},
  issn = {8756-3282},
  doi = {10.1016/8756-3282(96)00107-X},
  abstract = {The relationship of lactase malabsorption to osteoporosis is unclear. We examined the relationship of self-reported lactose intolerance (LI) to bone mineral density (BMD) in perimenopausal finnish women. A random population sample of 2025 women aged 48\textendash 59, who underwent spinal and femoral BMD measurement with dual X-ray absorptiometry in Kuopio, Finland during 1989\textendash 1991 formed the study population. Out of these women, 162 women reported LI. The mean dairy calcium intake was 558 mg/day in women with LI and 828 mg/day in other women (p {$<$} 0.0001). The mean spinal BMDs were 1.097 and 1.129 g/cm2 (-2.8\%) (p = 0.016) and the mean femoral BMDs were 0.906 and 0.932 g/cm2 (-2.8\%) (p = 0.012) for the LI and other women, respectively. After adjusting for weight, age, years since menopause, and the history of hormone replacement therapy, these differences changed to -2.7\% (p = 0.016) for the spinal and -2.4\% (p = 0.012) for the femoral BMD, respectively. Dairy calcium intake was an independent determinant of femoral BMD. The addition of calcium intake variables into the multivariate model did not affect the spinal BMD difference, but weakened the femoral BMD difference to -1.9\% (p = 0.075). Our results suggest that LI slightly reduces perimenopausal BMD, possibly through reduced calcium intake.},
  langid = {english},
  keywords = {Bone density,Calcium intake,Lactose intolerance,Lactose malabsorption,Menopause,Osteoporosis},
  file = {D\:\\Zotero\\storage\\6LQNDQFT\\875632829600107X.html}
}

@article{honkanenLactoseIntoleranceAssociated1997,
  title = {Lactose Intolerance Associated with Fractures of Weight-Bearing Bones in Finnish Women Aged 38\textendash 57 Years},
  author = {Honkanen, R. and Kr{\"o}ger, H. and Alhava, E. and Turpeinen, P. and Tuppurainen, M. and Saarikoski, S.},
  year = {1997},
  month = dec,
  journal = {Bone},
  volume = {21},
  number = {6},
  pages = {473--477},
  issn = {8756-3282},
  doi = {10.1016/S8756-3282(97)00172-5},
  abstract = {Lactose intolerance (LI) often results in decreased calcium intake. To test if long-term low intake of calcium affects bone strength, we examined fracture risks related to LI in women aged 38\textendash 57 years. The 11,619 Finnish women aged 47\textendash 56 years who responded to the baseline postal inquiry of the Kuopio Osteoporosis Risk Factor and Prevention Study in 1989 formed the study population. In all, 896 women reported LI and 1299 women reported a fracture in 1980\textendash 1989. Current intake of dairy calcium was lower in women with LI (570 mg/d) than in the other women (850 mg/d) (p {$<$} 0.0001). The fracture risk in general was slightly elevated in women with LI compared with the other women, with an odds ratio (OR) (95\% CI) of 1.33 (1.09\textendash 1.62). However, the fractures at the three most common sites (wrist, ankle, and rib) were not related to LI. In contrast, fractures at the tibia and metatarsal were strongly related to LI with ORs of 3.31 (1.51\textendash 7.24) and 2.84 (1.47\textendash 5.50), respectively. The adjusted OR for nonankle lower body fractures combined was 2.15 (1.53\textendash 3.04), whereas that for all upper body fractures combined was 1.15 (0.88\textendash 1.54). The 10 women with LI and a tibial or metatarsal fracture showed a 19\% lower femoral BMD than all the other women in the densitometry subsample of 3222 women (p {$<$} 0.001). Long-term premenopausal calcium deficiency differentially affects bones with weight-bearing nonankle bones being at the greates risk of suffering reduced strength.},
  langid = {english},
  keywords = {Calcium,Fracture,Lactose intolerance,Osteoporosis},
  file = {D\:\\Zotero\\storage\\XYXUL2XA\\S8756328297001725.html}
}

