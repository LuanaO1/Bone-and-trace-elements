@article{ringeEfficacyEtidronateSequential2005,
  title = {Efficacy of Etidronate and Sequential Monofluorophosphate in Severe Postmenopausal Osteoporosis: A Pilot Study},
  shorttitle = {Efficacy of Etidronate and Sequential Monofluorophosphate in Severe Postmenopausal Osteoporosis},
  author = {Ringe, J. D. and Dorst, A. and Faber, H. and Kipshoven, C. and Rovati, L. C. and Setnikar, I.},
