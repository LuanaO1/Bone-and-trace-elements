@article{pellegrinoDualenergyXrayAbsorptiometry2019,
  title = {Dual-Energy {{X-ray}} Absorptiometry Pitfalls in {{Thalassemia Major}}},
  author = {Pellegrino, Fabio and Zatelli, Maria Chiara and Bondanelli, Marta and Carnevale, Aldo and Cittanti, Corrado and Fortini, Monica and Gamberini, Maria Rita and Giganti, Melchiore and Ambrosio, Maria Rosaria},
