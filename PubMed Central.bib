}

@article{maricLIFESTYLERISKFACTORS2019,
  title = {{{LIFESTYLE RISK FACTORS AND BONE MASS IN RECURRENT STONE-FORMING PATIENTS}}: {{A CROSS-SECTIONAL STUDY IN}} 144 {{SUBJECTS}}},
  shorttitle = {{{LIFESTYLE RISK FACTORS AND BONE MASS IN RECURRENT STONE-FORMING PATIENTS}}},
